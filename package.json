{
  "name": "vue3-ts-vite-ssr-starter",
  "version": "1.0.0",
  "description": "Use vue3 vite typescript eslint SSR vuex vue-router element-plus scss",
  "author": "vok123",
  "scripts": {
    "dev": "npm-run-all -p start lint",
    "start": "vite",
    "lint": "esw -w --cache --color --ext .ts,.vue,.tsx src/ --cache-location node_modules",
    "build": "node build.js",
    "serve": "node dist/server/entry-server",
    "server:data": "node server-data.js",
    "test": "npm-run-all -s build serve"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "element-plus": "^1.0.1-alpha.14",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-rc.2",
    "vuex": "^4.0.0-rc.1"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-replace": "^2.3.3",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/server-renderer": "^3.0.0",
    "eslint": "^7.11.0",
    "eslint-plugin-vue": "^7.1.0",
    "eslint-watch": "^7.0.0",
    "express": "^4.17.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.28.0",
    "typescript": "^4.0.3",
    "vite": "^1.0.0-rc.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vok123/vue3-ts-vite-ssr-starter.git"
  },
  "license": "MIT"
}
